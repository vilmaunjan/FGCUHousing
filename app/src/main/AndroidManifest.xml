<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vilma.fgcuhousing">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ResidentAccess"
            android:label="@string/title_activity_resident_access"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ResidentRegister"
            android:label="@string/title_activity_resident_register"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ResidentLogin"
            android:label="@string/title_activity_resident_login"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EventList"
            android:label="@string/title_activity_event_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EventPage"
            android:label="@string/title_activity_event_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RatingPage"
            android:label="@string/title_activity_rating_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ResidentAccount"
            android:label="@string/title_activity_resident_account"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Awards"
            android:label="@string/title_activity_awards"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EventManager"
            android:label="@string/title_activity_event_manager"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CreateEvent"
            android:label="@string/title_activity_create_event"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AwardInfo"
            android:label="AwardInfo"
            android:theme="@style/AppTheme.AwardTheme" />
        <activity
            android:name=".EventEdit"
            android:label="@string/title_activity_event_edit"
            android:parentActivityName=".EventManager"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.vilma.fgcuhousing.EventManager" />
        </activity>
        <activity
            android:name=".EventFeedback"
            android:label="@string/title_activity_event_feedback"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Stats"
            android:label="@string/title_activity_stats"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>